{
  "name": "create-expressr-app",
  "version": "1.0.2",
  "description": "Create Express apps with dynamic folder routing",
  "main": "bin/create-expressr-app.js",
  "bin": {
    "create-expressr-app": "./bin/create-expressr-app.js"
  },
  "files": [
    "bin/",
    "templates/",
    "templates/src/",
    "templates/src/routes/",
    "templates/src/routes/examples/",
    "templates/src/routes/(api)/",
    "templates/src/utils/"
  ],
  "scripts": {
    "prepublishOnly": "chmod +x bin/create-expressr-app.js"
  },
  "keywords": [
    "express",
    "typescript",
    "routing",
    "create-app",
    "cli"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20.10.4",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.3.3"
  }
}
